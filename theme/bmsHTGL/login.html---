<body style="background:#e7f1fc">
<form id="loginForm" class="loginBg">
	<div class="loginBox">
    	<div class="login">
        <div class="loginTable">
        <div id="logo"><img width="340" height="110" src="images/cms_newlogo.jpg"></div>
        	<table width="100%" border="0" cellspacing="0" cellpadding="0">
              <tr>
              	<td align="right"></td>
                <td><span class="lgntext"><span class="user"></span><input type="text" id="userLoginName" name="userLoginName" value="登录帐号" /></span></td>
              </tr>
              <tr>
              	<td align="right"></td>
                <td><span class="lgntext"><span class="password"></span><input type="password" autocomplete="off" id="userLoginPassword" name="userLoginPassword" value="" /><input type="text" value="登录密码" id="passshow" name="pwdshow"></span></td>
              </tr>
              <tr id="useValidCode">
              	<td align="right"></td>
                <td class="middle"><span class="lgntext2"><span class="code"></span><input type="text" id="validCode" maxlength="5" name="validCode" value="验证码" /></span><img id="validCodeImg" src="common/sinoaptcha1.jpg"  width="77" height="22" class="ml10 hand" /></td>
              </tr>
              <tr id="phoneValidate" class="undis">
              	<td align="right"></td>
                <td class="middle"><span class="lgntext2"><input type="text" id="phoneValidateNum" name="phoneValidateNum" maxlength="6" onkeyup="value=value.replace(/[^\d]/g,'')" 
									    onbeforepaste="clipboardData.setData('text',clipboardData.getData('text').replace(/[^\d]/g,''))" /></span><a href="javascript:;" style="color:#fff; display:inline-block;width:100px; height:24px; line-height:24px; text-align:center;background:#e71600;" class="ml10 btn_href"  id="get_">获取短信验证码</a></td>
              </tr>
              <tr>
              <td align="right">&nbsp;</td>
                <td><a href="index.html" id="submit" class="loginBtn font18 mr20 btn_href">登录</a><a href="javascript:;" id="reset" class="resetBtn font18 btn_href">重置</a></td>
              </tr>
            </table>
        </div>
        </div>
    </div>
</form>
<!--弹出层 开始-->
<div class="cover" id="cover"></div>
<!--保存成功 开始-->
<div class="pop" id="bccg" style=" width:300px;">
	<div class="popTitle"><a href="javascript:;" id="closeUrl" class="close fr btn_href" onclick="hide('cover','bccg')"></a><strong class="font14 fl">提示</strong><div class="clear"></div></div>
	<div class="mt25 mb25">
    	<p class="text_c" id="message">保存成功！</p>
    </div>
	<div class="text_c mt10 mb10"><a href="javascript:;" class="mr10 bt_blue btn_href" onclick="hide('cover','bccg')" id="goUrl"><span>确定</span></a></div>
</div>
<!--保存成功 结束-->
</body>
</html>
